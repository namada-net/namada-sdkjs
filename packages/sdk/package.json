{
  "name": "@namada/sdk",
  "version": "0.22.0",
  "description": "Namada Web SDK",
  "main": "dist/sdk/src/index.js",
  "types": "dist/sdk/src/index.d.ts",
  "repository": "https://github.com/anoma/namada-sdkjs/",
  "author": "Heliax Dev <info@heliax.dev>",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/sdk/src/index.js",
      "types": "./dist/sdk/src/index.d.ts"
    },
    "./init": {
      "import": "./dist/sdk/src/init.js",
      "types": "./dist/sdk/src/init.d.ts"
    },
    "./inline": {
      "import": "./dist/sdk/src/initInline.js",
      "types": "./dist/sdk/src/initInline.d.ts"
    }
  },
  "files": [
    "dist/lib/**/*.{js,ts}",
