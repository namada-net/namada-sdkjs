    "dist/wasm/**/*.{js,ts,wasm}",
    "dist/sdk.namada.wasm"
  ],
  "dependencies": {
    "@cosmjs/encoding": "^0.29.0",
    "@dao-xyz/borsh": "^5.1.5",
    "@ledgerhq/hw-transport": "^6.31.4",
    "@ledgerhq/hw-transport-webusb": "^6.29.4",
    "@zondax/ledger-namada": "^2.0.0",
    "bignumber.js": "^9.1.1",
    "buffer": "^6.0.3",
    "semver": "^7.6.3",
    "slip44": "^3.0.18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
