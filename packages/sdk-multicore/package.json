{
  "name": "@namada/sdk-multicore",
  "version": "0.20.0",
  "description": "Namada Web Multicore SDK",
  "main": "sdk-multicore/src/index.js",
  "types": "sdk-multicore/src/index.d.ts",
  "repository": "https://github.com/anoma/namada-sdkjs/",
  "author": "Heliax Dev <info@heliax.dev>",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./sdk-multicore/src/index.js",
      "types": "./sdk-multicore/src/index.d.ts"
    },
    "./init": {
      "import": "./sdk-multicore/src/init.js",
      "types": "./sdk-multicore/src/init.d.ts"
    },
    "./inline": {
      "import": "./sdk-multicore/src/initInline.js",
      "types": "./sdk-multicore/src/initInline.d.ts"
    },
    "./wasm/src/sdk": {
      "import": "./wasm/src/sdk/sdk.js",
      "types": "./wasm/src/sdk/sdk.d.ts"
    }
  },
  "files": [
    "lib/**/*.{js,ts}",
    "sdk-multicore/**/*.{js,ts}",
    "wasm/**/*.{js,ts,wasm}",
    "sdk.namada.wasm"
  ],
  "scripts": {
    "build": "tspc -p ./tsconfig.json"
  },
  "dependencies": {
    "@cosmjs/encoding": "^0.29.0",
    "@dao-xyz/borsh": "^5.1.5",
    "@ledgerhq/hw-transport": "^6.31.4",
    "@ledgerhq/hw-transport-webusb": "^6.29.4",
    "@zondax/ledger-namada": "^2.0.0",
    "bignumber.js": "^9.1.1",
    "buffer": "^6.0.3",
    "semver": "^7.7.2",
    "slip44": "^3.0.18"
  },
  "devDependencies": {
    "ts-patch": "^3.1.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": [
    "./wasm/src/sdk/snippets/*"
  ]
}
